@model CityBusManagement.Entity.Models.BusDetails

<form enctype="multipart/form-data">
    <h2>DELETE BUS DETAILS</h2>

    <hr />
    @if (ViewBag.status == "Ok")
    {
        <div class="alert alert-success">
            @ViewBag.message
        </div>
    }
    else if (ViewBag.status == "Error")
    {
        <div class="alert alert-danger">
            @ViewBag.message
        </div>
    }

</form>

<div>
    <a asp-controller="BusDetails" asp-action="BusDetails">View  List</a>
</div>